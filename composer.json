{
    "name": "kartik-v/yii2-mail-manager",
    "description": "Yii2 mail management module that allows DB queuing, monitor email queues, templates, setup mail categories, and manage scheduling via cron.",
    "keywords": [
        "yii2",
        "extension",
        "widget",
        "mail-manager"
    ],
    "homepage": "https://github.com/kartik-v/yii2-mail-manager",
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Kartik Visweswaran",
            "email": "kartikv2@gmail.com",
            "homepage": "http://www.krajee.com/"
        }
    ],
    "require": {
        "php": ">=5.5",
        "yiisoft/yii2-swiftmailer": "~2.0",
        "kartik-v/yii2-widget-activeform": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "kartik\\mailmanager\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "bootstrap": "kartik\\mailmanager\\Bootstrap"
    }
}